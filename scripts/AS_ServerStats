./plotDiskRTP.py -i dstat.e2 -o DiskRTP_AS_e2.png -d aerospike
./plotDiskRIOPS.py -i dstat.e2 -o DiskRIOPS_AS_e2.png -d aerospike
./plotDiskWIOPS.py -i dstat.e2 -o DiskWIOPS_AS_e2.png -d aerospike
./plotNetRTP.py -i dstat.e2 -o NetRTP_AS_e2.png -d aerospike
./plotNetWTP.py -i dstat.e2 -o NetWTP_AS_e2.png -d aerospike
./plotCPUIdle.py -i dstat.e2 -o CPU_AS_e2.png -d aerospike

./plotDiskWTP.py -i dstat.e3 -o DiskWTP_AS_e3.png -d aerospike
./plotDiskRTP.py -i dstat.e3 -o DiskRTP_AS_e3.png -d aerospike
./plotDiskRIOPS.py -i dstat.e3 -o DiskRIOPS_AS_e3.png -d aerospike
./plotDiskWIOPS.py -i dstat.e3 -o DiskWIOPS_AS_e3.png -d aerospike
./plotNetRTP.py -i dstat.e3 -o NetRTP_AS_e3.png -d aerospike
./plotNetWTP.py -i dstat.e3 -o NetWTP_AS_e3.png -d aerospike
./plotCPUIdle.py -i dstat.e3 -o CPU_AS_e3.png -d aerospike

./plotDiskWTP.py -i dstat.e4 -o DiskWTP_AS_e4.png -d aerospike
./plotDiskRTP.py -i dstat.e4 -o DiskRTP_AS_e4.png -d aerospike
./plotDiskRIOPS.py -i dstat.e4 -o DiskRIOPS_AS_e4.png -d aerospike
./plotDiskWIOPS.py -i dstat.e4 -o DiskWIOPS_AS_e4.png -d aerospike
./plotNetRTP.py -i dstat.e4 -o NetRTP_AS_e4.png -d aerospike
./plotNetWTP.py -i dstat.e4 -o NetWTP_AS_e4.png -d aerospike
./plotCPUIdle.py -i dstat.e4 -o CPU_AS_e4.png -d aerospike
